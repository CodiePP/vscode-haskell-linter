{
	"name": "haskell-linter",
	"displayName": "haskell-linter",
	"description": "An extension to use hlint in vscode",
	"version": "0.0.1",
	"publisher": "hoovercj",
	"license": "SEE LICENSE IN LICENSE.txt",
	"homepage": "https://github.com/hoovercj/vscode-haskell-linter",
	"repository": {
		"type": "git",
		"url": "https://github.com/hoovercj/vscode-haskell-linter.git"
	},
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Linter"
	],
	"activationEvents": [
		"onLanguage:haskell"
	],
	"main": "./out/src/extension",
	"contributes": {
		"languages": [
			{
				"id": "haskell",
				"aliases": ["Haskell", "haskell"],
				"extensions": [".hs"]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	},
	"properties": {
		"haskell.validate.executablePath": {
			"type": "string",
			"default": null,
			"description": "Points to the hlint exectuable."
		}
	}
}